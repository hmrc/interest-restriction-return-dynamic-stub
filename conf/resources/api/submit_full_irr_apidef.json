      "correlationId": {
        "in": "header",
        "name": "CorrelationId",
        "description": "Optional. A UUID format string for the transaction. If not specified the IF will create a UUID value to be used",
        "required": false,
        "schema": {
          "type": "string",
          "pattern": "^[0-9a-fA-F]{8}[-][0-9a-fA-F]{4}[-][0-9a-fA-F]{4}[-][0-9a-fA-F]{4}[-][0-9a-fA-F]{12}$"
